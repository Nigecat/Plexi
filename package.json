{
    "name": "plexi",
    "version": "5.5.3",
    "description": "A general purpose discord bot",
    "author": "Nigecat",
    "repository": {
        "type": "git",
        "url": "https://github.com/Nigecat/Plexi.git"
    },
    "scripts": {
        "lint": "eslint . --ext .ts --max-warnings 0",
        "lint:fix": "eslint . --ext .ts --fix",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "build:clean": "tsc --build --clean",
        "build:docs": "npm run build && ts-node generateDocs.ts",
        "start": "npm run build && node -r dotenv/config index.js",
        "start:dev": "docker-compose up --build",
        "start:nobuild": "node -r dotenv/config index.js",
        "tests": "npm run build:clean && mocha -r ts-node/register tests/**/*.test.ts",
        "test": "npm run build && npm run tests && npm run lint",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/common-tags": "^1.8.0",
        "@types/mathjs": "^6.0.5",
        "@types/mocha": "^8.0.3",
        "@types/mongoose": "^5.7.36",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "chai": "^4.2.0",
        "conventional-changelog-cli": "^2.1.0",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "mocha": "^8.1.2",
        "prettier": "^2.1.1",
        "ts-node": "^9.0.0"
    },
    "dependencies": {
        "@discordjs/opus": "^0.3.2",
        "@types/node": "^14.6.0",
        "common-tags": "^1.8.0",
        "dblapi.js": "^2.4.1",
        "discord.js": "^12.3.1",
        "dotenv": "^8.2.0",
        "ffmpeg-static": "^4.2.7",
        "jimp": "^0.16.0",
        "mathjs": "^7.2.0",
        "mongoose": "^5.10.1",
        "popyt": "^4.1.1",
        "typescript": "^4.0.2",
        "weather-js": "^2.0.0",
        "winston": "^3.3.3",
        "word-definition": "^2.1.6",
        "ytdl-core-discord": "^1.2.1"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.1",
        "erlpack": "github:discord/erlpack",
        "libsodium-wrappers": "^0.7.8",
        "utf-8-validate": "^5.0.2",
        "zlib-sync": "^0.1.7"
    }
}
