{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "no-console": "warn",
        "eqeqeq": "error",
        "spaced-comment": "warn",
        "no-loss-of-precision": "error",
        "generator-star-spacing": "warn"
    }
}